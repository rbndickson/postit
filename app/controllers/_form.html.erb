<%= render 'shared/errors', submitted_form: @post %>


<%= form_for @post do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br/>
  <%= f.label :url %>
  <%= f.text_field :url %>
  <br/>
  <%= f.label :description %>
  <%= f.text_area :description, :rows => 4 %>
  <br/>
  <%= f.label :user_id %>
  <%= f.text_field :user_id %>
  <br/>
  <%= f.submit button_text, class: 'btn btn-primary' %>
<% end %>
